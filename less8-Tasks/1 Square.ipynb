    "import math\n",
    "\n",
    "def square(a):\n",
    "    p = 4*a\n",
    "    s = a**2\n",
    "    d = a*math.sqrt(2)\n",
    "    return (p,s,d)\n",
    "\n",
    "p,s,d = square(3)\n",
    "print(\"Perimeter: {p:1.2f}\\nSquare: {s:1.2f}\\nDiagonal: {d:1.2f}\".format(p=p,s=s,d=d))"
   ]
